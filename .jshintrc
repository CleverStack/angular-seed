{
  "maxerr": 100,
  "passfail": false,
  "smarttabs": true,
  "browser": true,
  "predef": [
    "__dirname",
    "define",
    "describe",
    "it",
    "xit",
    "beforeEach",
    "afterEach",
    "expect",
    "spyOn",
    "runs",
    "waits",
    "waitsFor"
  ],
  "globals": {
    "require": true,
    "module": true,
    "sinon": true,
    "S": true,
    "steal": true,
    "test": true,
    "equal": true,
    "ok": true,
    "console": true,
    "process": true
